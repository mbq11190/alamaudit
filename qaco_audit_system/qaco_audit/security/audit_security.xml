<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_qaco_audit_user" model="res.groups">
        <field name="name">Audit User</field>
        <field name="category_id" ref="base.module_category_accounting"/>
    </record>
    <record id="group_qaco_audit_manager" model="res.groups">
        <field name="name">Audit Manager</field>
        <field name="implied_ids" eval="[(4, ref('group_qaco_audit_user'))]"/>
        <field name="category_id" ref="base.module_category_accounting"/>
    </record>
    <record id="group_qaco_audit_partner" model="res.groups">
        <field name="name">Audit Partner</field>
        <field name="implied_ids" eval="[(4, ref('group_qaco_audit_user')), (4, ref('group_qaco_audit_manager'))]"/>
        <field name="category_id" ref="base.module_category_accounting"/>
    </record>
    <record id="group_qaco_audit_eqcr" model="res.groups">
        <field name="name">EQCR Reviewer</field>
        <field name="category_id" ref="base.module_category_accounting"/>
    </record>
</odoo>