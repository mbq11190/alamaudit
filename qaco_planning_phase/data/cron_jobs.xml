<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_pbc_escalation" model="ir.cron">
            <field name="name">PBC Escalation</field>
            <field name="model_id" ref="model_qaco_planning_pbc"/>
            <field name="state">code</field>
            <field name="code">model.cron_escalate_overdue()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
