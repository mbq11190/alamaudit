        <!-- Sequence for Planning Phase -->
        <record id="seq_planning_phase" model="ir.sequence">
            <field name="name">Planning Phase Sequence</field>
            <field name="code">qaco.planning.phase</field>
            <field name="prefix">PLAN/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
