<odoo>
  <data noupdate="1">
    <record id="seq_planning_phase" model="ir.sequence">
      <field name="name">Audit Planning Phase</field>
      <field name="code">qaco.planning.phase</field>
      <field name="prefix">PLAN-%(y)s-</field>
      <field name="padding">4</field>
      <field name="company_id" eval="False"/>
      <field name="implementation">no_gap</field>
    </record>
    <record id="seq_materiality" model="ir.sequence">
      <field name="name">Materiality Worksheet</field>
      <field name="code">qaco.materiality</field>
      <field name="prefix">MAT-%(y)s-</field>
      <field name="padding">4</field>
      <field name="company_id" eval="False"/>
      <field name="implementation">no_gap</field>
    </record>
    <record id="seq_planning_risk" model="ir.sequence">
      <field name="name">Planning Risk</field>
      <field name="code">qaco.planning.risk</field>
      <field name="prefix">RISK-%(y)s-</field>
      <field name="padding">4</field>
      <field name="company_id" eval="False"/>
      <field name="implementation">no_gap</field>
    </record>
    <record id="seq_risk_matrix_fs" model="ir.sequence">
      <field name="name">FS Risk Matrix</field>
      <field name="code">qaco.risk_matrix.fs</field>
      <field name="prefix">FSR-%(y)s-</field>
      <field name="padding">4</field>
      <field name="company_id" eval="False"/>
      <field name="implementation">no_gap</field>
    </record>
  </data>
</odoo>
