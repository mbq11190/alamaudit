<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="sequence_transfer" model="ir.sequence">
            <field name="name">Transfer Sequence</field>
            <field name="code">transfer.sequence</field>
            <field name="prefix">TRF/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_leave_summary" model="ir.sequence">
            <field name="name">Leave Summary Sequence</field>
            <field name="code">leave.summary.seq</field>
            <field name="prefix">LVS/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_leave_adjustment" model="ir.sequence">
            <field name="name">Leave Adjustment Sequence</field>
            <field name="code">leave.adjustment.seq</field>
            <field name="prefix">LVA/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
