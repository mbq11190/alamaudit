<odoo>
  <data noupdate="1">

    <record id="ir_cron_daily_attendance_report" model="ir.cron">
      <field name="name">Daily Attendance Report</field>
      <field name="model_id" ref="model_daily_attendance_report"/>
      <field name="state">code</field>
      <field name="code">model.generate_daily_attendance_report()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="numbercall">-1</field>
      <field name="active" eval="True"/>
      <field name="user_id" ref="base.user_admin"/>
    </record>

  </data>
</odoo>
