<odoo>
    <data noupdate="1">
        <!-- Weekly Cron Job for Employee Report -->
        <record id="ir_cron_weekly_employee_report" model="ir.cron">
            <field name="name">Generate Weekly Employee Report</field>
            <field name="model_id" ref="qaco_employees.model_weekly_employee_report"/>
            <field name="state">code</field>
            <field name="code">model.generate_employee_report()</field>
            <field name="user_id" ref="base.user_admin"/>
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
